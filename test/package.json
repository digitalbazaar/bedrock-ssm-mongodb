    "bedrock-ssm-mongodb": "file:..",
    "bedrock-test": "^6.0.0",
    "bnid": "^2.0.0",
    "cross-env": "^7.0.2",
    "crypto-ld": "^6.0.0",
    "nyc": "^15.1.0"
